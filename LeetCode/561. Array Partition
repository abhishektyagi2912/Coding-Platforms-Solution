class Solution {
public:
    int arrayPairSum(vector<int>& nums) {
        int sum=0;
        sort(nums.begin(),nums.end());
        for(int i=0;i<nums.size();i+=2){ //because i ko har baar 2 number s jump kra do because humesha 1st element is minimum 
            sum+=nums[i];
        }
        return sum;
    }
};
