class Solution {
public:
    int search(vector<int>& nums, int target) {
        int i=0;
        int e=nums.size;
        while(i<=e){
            int mid=(i+e)/2;
            if(nums[mid]==target) return mid;
            else if(nums[mid]>target) return e=mid-1;
            else i=mid+1;
        }
        
        return -1
    }
};
